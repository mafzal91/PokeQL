{
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "extends": ["eslint:recommended", "eslint-config-prettier"],
  "rules": {
    "strict": ["error", "never"],
    "sort-keys": ["error", "asc", {"natural": true}],
    "no-use-before-define": ["error"],
    "no-unused-vars": ["warn"]
  },
  "globals":{
    "fetch": "readonly",
    "document": "readonly"
  },
  "env": {
    "node": true,
    "es6": true
  }
}
